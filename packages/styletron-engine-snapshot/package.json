{
  "name": "styletron-engine-snapshot",
  "version": "0.1.4",
  "description": "Universal, high-performance JavaScript styles",
  "author": "Mo Kouli <mohamad.kouli@gmail.com>",
  "repository": "styletron/styletron",
  "sideEffects": false,
  "files": [
    "lib",
    "dist-browser-cjs",
    "dist-browser-esm",
    "dist-node-cjs",
    "dist-node-esm",
    "src",
    "!__tests__"
  ],
  "main": "./dist-node-cjs/index.js",
  "module": "./dist-node-esm/index.js",
  "types": "./lib/index.d.ts",
  "browser": {
    "./dist-node-cjs/index.js": "./dist-browser-cjs/index.js",
    "./dist-node-esm/index.js": "./dist-browser-esm/index.js"
  },
  "scripts": {
    "build": "cup build --skip-flow && yarn libdefs",
    "clean": "rimraf dist* && rimraf lib && rimraf tsconfig.tsbuildinfo",
    "libdefs": "yarn api-extractor run && yarn flowgen dist/index.d.ts --add-flow-header -o dist-node-cjs/index.js.flow",
    "pretest": "cup build-tests",
    "test": "unitest --node=dist-tests/node.js --browser=dist-tests/browser.js",
    "prepublish": "npm run build"
  },
  "dependencies": {
    "json-stable-stringify": "^1.0.1",
    "prettier": "^2.0.5"
  },
  "peerDependencies": {
    "styletron-standard": "^3.0.1"
  },
  "devDependencies": {
    "@miksu/create-universal-package": "0.0.2",
    "styletron-standard": "^3.0.4",
    "tape": "4.9.1",
    "unitest": "^2.1.1"
  },
  "license": "MIT"
}
