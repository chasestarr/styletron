{
  "name": "styletron-react",
  "version": "5.2.8",
  "description": "React bindings for Styletron",
  "author": "Ryan Tsao <ryan.j.tsao@gmail.com>",
  "repository": "styletron/styletron",
  "sideEffects": false,
  "files": [
    "dist",
    "dist-browser-cjs",
    "dist-browser-esm",
    "dist-node-cjs",
    "dist-node-esm",
    "src",
    "!__tests__"
  ],
  "main": "./dist-node-cjs/index.js",
  "module": "./dist-node-esm/index.js",
  "types": "./dist/index.d.ts",
  "browser": {
    "./dist-node-cjs/index.js": "./dist-browser-cjs/index.js",
    "./dist-node-esm/index.js": "./dist-browser-esm/index.js"
  },
  "scripts": {
    "build": "cup build --skip-flow && yarn libdefs",
    "libdefs": "yarn tsc && yarn api-extractor run && yarn flowgen dist/index.d.ts -o dist-node-cjs/index.js.flow",
    "pretest": "cup build-tests",
    "test": "../../node_modules/.bin/unitest --browser=dist-tests/browser.js --node=dist-tests/node.js"
  },
  "dependencies": {
    "prop-types": "^15.6.0",
    "styletron-standard": "^3.0.4"
  },
  "peerDependencies": {
    "react": "^16.8.0"
  },
  "devDependencies": {
    "@types/react": "^16.8.23",
    "@types/webpack-env": "^1.13.9",
    "@babel/preset-react": "^7.10.4",
    "@miksu/create-universal-package": "0.0.2",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint-plugin-react": "^7.6.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "unitest": "^2.1.1"
  },
  "license": "MIT"
}
